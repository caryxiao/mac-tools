# , .editorconfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true
indent_style = space

[**.js]
indent_size = 2

[**.html]
indent_size = 2

[**.css]
indent_size = 2

[**.py]
indent_size = 4
insert_final_newline = false

[**.php]
indent_size = 4

[**.go]
indent_size = 4
